<h1>Listing contract_services</h1>

<table>
  <thead>
    <tr>
      <th>Contract</th>
      <th>Service type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contract_services.each do |contract_service| %>
      <tr>
        <td><%= contract_service.contract %></td>
        <td><%= contract_service.service_type %></td>
        <td><%= link_to 'Show', contract_service %></td>
        <td><%= link_to 'Edit', edit_contract_service_path(contract_service) %></td>
        <td><%= link_to 'Destroy', contract_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract service', new_contract_service_path %>
