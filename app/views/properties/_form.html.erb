<%= form_for @property, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <%= f.label :address1, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :address1, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :address2, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :address2, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :city, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :city, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :state, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :state, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :latitude, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :latitude, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :longitude, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :longitude, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :altitude, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :altitude, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :country, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :country, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :zipcode, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :zipcode, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :size_in_acres, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :size_in_acres, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :length_in_feet, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :length_in_feet, :class => 'text-field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :width_in_feet, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :width_in_feet, :class => 'text-field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  properties_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
